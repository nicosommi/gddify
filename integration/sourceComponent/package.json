{
  "dependencies": {
    "shelljs": "^0.7.5"
  },
  "devDependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.9.0",
    "babel-require": "^1.0.1",
    "mocha": "^2.5.3",
    "should": "^9.0.2",
    "watch": "^0.19.1",
    "nyc": "^10.0.0",
    "standard": "^8.6.0",
    "proxyquire": "^1.7.10",
    "bluebird": "^3.3.5",
    "debug": "^2.2.0"
  },
  "scripts": {
    "pretest": "gddify refresh",
    "test": "standard --fix && nyc --reporter=text-summary mocha --require babel-core/register 'test/**/*.test.js'",
    "watch": "watch 'nyc --reporter=text-summary mocha --require babel-core/register \"test/**/*.test.js\"' src test",
    "coverage": "nyc mocha --require babel-core/register 'test/**/*.test.js'",
    "fix": "standard --fix"
  },
  "author": "nicosommi",
  "license": "MIT",
  "name": "daydo",
  "version": "0.0.0",
  "description": "Daydo",
  "bin": "dist/bin/daydo.js",
  "directories": {
    "test": "test"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ]
  },
  "nyc": {
    "lines": 60,
    "statements": 60,
    "functions": 60,
    "branches": 60,
    "sourceType": "module",
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.test.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "require": [
      "babel-register"
    ],
    "extension": [
      ".js"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./.coverage"
  },
  "standard": {
    "ignore": [],
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "xit",
      "expect"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.com/nicosommi/daydo.git"
  }
}
